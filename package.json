{
  "name": "mogo",
  "version": "1.0.0",
  "description": "A demo to use mongodb.atlast",
  "main": ".compiled/app.js",
  "scripts": {
    "start": "node .compiled/app.js",
    "test": "mocha -- .compiled/**/test/*.js --recursive ||true",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive",
    "coverage-report": "start chrome coverage/lcov-report/index.html",
    "compile": "babel src --out-dir .compiled --source-maps --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neotechmonk/mogo-atlas.git"
  },
  "keywords": [
    "MongoDB",
    "Atlas"
  ],
  "author": "Theepan Thevathasan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neotechmonk/mongo-atlas/issues"
  },
  "homepage": "https://github.com/neotechmonk/mongo-atlas#readme",
  "devDependencies": {
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongoose": "^5.3.2"
  }
